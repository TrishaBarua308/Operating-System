echo "Input a b c"
read a
read b 
read c 
if [ $a -ge $b ]
then 
    if [ $a -ge $c ]
    then
         echo "$a is larger"
    else
         echo "$c is larger"
    fi 
else
    if [ $b -ge $c ]
    then
         echo "$b is larger"
    else
         echo "$c is larger"
    fi
fi
